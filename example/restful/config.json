{
  "server": {
    "type": "default_server",
    "applicationConnectors": [
      {
        "type": "http",
        "addr": ":8080"
      }
    ],
    "adminConnectors": [
      {
        "type": "http",
        "addr": ":8081"
      }
    ]
  },
  "logging": {
    "level": "DEBUG",
    "loggers": {
      "gomelon/server": "INFO",
      "gomelon/configuration": "WARN"
    },
    "appenders": [
      {
	"type": "console_appender"
      }
    ]
  },
  "metrics": {
    "frequency": "1s"
  }
}
